{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.{js,mjs,jsx,cjs,ts,tsx}": [
      "@parcel/transformer-js",
      "@parcel/transformer-react-refresh-wrap"
    ]
  },
  "validators": {
    "./src/**/*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "@parcel/validator-eslint"
    ]
  }
}
